{
  "name": "nestjs-boilerplate",
  "version": "1.0.0",
  "scripts": {
    "build": "nest build",
    "start": "node dist/main.js",
    "start:dev": "nest start --watch",
    "migration:generate": "node scripts/migration-generate.js",
    "migration:run:dev": "npx typeorm-ts-node-commonjs -d src/config/typeorm.config.ts migration:run",
    "migration:run:prod": "npx typeorm -d dist/config/typeorm.config.js migration:run",
    "migration:run": "if [ \"$NODE_ENV\" = \"production\" ]; then npm run migration:run:prod; else npm run migration:run:dev; fi"
  },
